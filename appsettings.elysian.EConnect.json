{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "System.Net.Http.HttpClient": "Warning"
    },
    "LoggerProviders": {
      "EnableKafka": false,
      "EnableConsole": false,
      "EnableColoredConsole": true
    },
    "Console": {},
    "ColoredConsole": {
      "Information": "Green",
      "Warning": "Yellow",
      "Error": "Red",
      "Critical": "DarkRed"
    }
  },
  "Kestrel": {
    "Certificates": {
      "Default": {
        "Path": "/run/secrets/elysian.pfx",
        "Password": "password"
      }
    }
  },
