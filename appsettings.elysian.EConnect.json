    "Kafka": { "ProducerTopic": "elysian.general" }
  },
  "Caching": {
    "CacheType": "Memory",
    "SlidingExpirationMs": "600000"
  },
  "Validation": {
    "SchemaPath": "./Schema/Json/"
  },
